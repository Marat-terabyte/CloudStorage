<Window x:Class="Client.View.CloudStorageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.View"
        xmlns:viewmodel="clr-namespace:Client.ViewModel"
        mc:Ignorable="d"
        Title="CloudStorage" Height="450" Width="800">
    <Window.DataContext>
        <viewmodel:CloudStorageViewModel></viewmodel:CloudStorageViewModel>
    </Window.DataContext>
    <Grid>
        <ContentControl Content="{Binding CurrentContent}"></ContentControl>
    </Grid>
</Window>
